proc rec_count_down(i)
	print "rec_count_down " i
	if i == 0 then
		return 0
	endif
	return rec_count_down(i - 1)
endproc

rec_count_down(10)

proc rec_count_down2(i)
	print "rec_count_down2 " i
	if i == 0 then
		return 0
	endif
	i = i - 1
	return rec_count_down(i)
endproc

rec_count_down(10)
rec_count_down2(10)
